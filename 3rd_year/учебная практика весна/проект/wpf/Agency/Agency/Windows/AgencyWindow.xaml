<Window x:Class="Agency.Windows.AgencyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Agency.Windows"
        mc:Ignorable="d"
        Title="Кабинет риэлтора" MinHeight="450" MinWidth="800" MaxHeight="550" MaxWidth="900" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button x:Name="BackBtn" Click="BackBtn_Click" Style="{StaticResource BackBtn}"></Button>
        <Image Height="45" Margin="10"  Grid.Column="0" Grid.Row="5" HorizontalAlignment="Left" Source="/Resources/logo (1).png"></Image>
        <Label Style="{StaticResource Title}" Content="Кабинет агента(риэлтора)"></Label>
        <Button x:Name="DealsBtn" Click="DealsBtn_Click" Content="Сделки" Grid.Row="4" Style="{StaticResource ActionBtn}"></Button>
        <Button x:Name="AgensBtn" Click="AgensBtn_Click" Style="{StaticResource ActionBtn}" Content="Риэлторы" Grid.Row="3"></Button>
        <Button x:Name="RealEstatesBtn" Click="RealEstatesBtn_Click" Style="{StaticResource ActionBtn}" Content="Недижимость" Grid.Row="2"></Button>
        <Button x:Name="ClientsBtn" Click="ClientsBtn_Click" Style="{StaticResource ActionBtn}" Content="Клиенты" Grid.Row="1"></Button>
    </Grid>
</Window>
